<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="10dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/litebackground"
        android:padding="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/txt_requestid_rr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:textSize="18sp" />

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/profile_pic_rr"
            android:layout_width="116dp"
            android:layout_height="129dp"


            android:layout_alignParentStart="true"

            android:paddingLeft="5dp"
            android:paddingTop="5dp"
            android:paddingRight="5dp"
            android:paddingBottom="5dp"
            android:scaleType="centerCrop"
            app:shapeAppearanceOverlay="@style/roundedImageViewRounded"
            app:strokeColor="@color/black" />

        <ImageView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="2dp"
            android:layout_toEndOf="@+id/profile_pic_rr"
            android:src="@drawable/baseline_account_circle_24" />

        <TextView
            android:id="@+id/txt_username_rr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/textView7"

            android:layout_alignBottom="@+id/textView7"
            android:layout_marginLeft="4dp"

            android:layout_toEndOf="@+id/textView7"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/profile_pic_rr"
            android:layout_alignParentStart="true"
            android:layout_marginTop="3dp"
            app:srcCompat="@drawable/pickup_location" />

        <TextView
            android:id="@+id/txt_pickup_rr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"


            android:layout_below="@+id/profile_pic_rr"
            android:layout_alignTop="@id/imageView3"
            android:layout_alignBottom="@id/imageView3"

            android:layout_toEndOf="@id/imageView3"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView3"
            android:layout_alignParentStart="true"
            app:srcCompat="@drawable/drop_location" />

        <TextView
            android:id="@+id/txt_dropoff_d"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_pickup_rr"
            android:layout_alignBottom="@+id/imageView4"

            android:layout_toEndOf="@+id/imageView4"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView7"
            android:layout_toEndOf="@+id/profile_pic_rr"
            android:src="@drawable/baseline_calendar_month_24"
            android:layout_marginTop="5dp"
            />

        <TextView
            android:id="@+id/txt_date_rr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_username_rr"
            android:layout_alignBottom="@+id/textView10"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/textView10"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView10"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/profile_pic_rr"
            android:src="@drawable/baseline_access_time_24"
            />

        <TextView
            android:id="@+id/txt_time_rr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_date_rr"
            android:layout_alignBottom="@+id/textView16"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/textView16"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/textView9"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_below="@+id/textView16"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/profile_pic_rr"
            android:src="@drawable/assigncar" />

        <TextView
            android:id="@+id/txt_serricetype_rr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_time_rr"
            android:layout_alignBottom="@+id/textView9"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/textView9"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/img_assign_rr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_dropoff_d"
            android:layout_alignParentEnd="true"
            app:srcCompat="@drawable/baseline_check_24" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_dropoff_d"
            android:layout_toStartOf="@+id/img_assign_rr"
            app:srcCompat="@drawable/baseline_close_24" />

        <TextView
            android:id="@+id/txt_price_rr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_serricetype_rr"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="2dp"
            android:layout_marginRight="4dp"
            android:textSize="18sp" />

        <Button
            android:id="@+id/btn_rr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_dropoff_d"
            android:layout_toStartOf="@+id/imageView6"
            android:text="Button" />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>